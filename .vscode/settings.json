{
  "editor.formatOnSave": true,
  "editor.defaultFormatter": "rome.rome",
  "[typescriptreact]": {
    "editor.defaultFormatter": "rome.rome"
  },
  "[typescript]": {
    "editor.defaultFormatter": "rome.rome"
  },
  "editor.codeActionsOnSave": {
    "source.organizeImports.rome": true,
    "quickfix.rome": true
  },
  "[jsonc]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  }
}